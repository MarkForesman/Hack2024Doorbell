# Example .env file for Doorbell device

# Unique identifier for this device
DEVICE_ID=device-101

# Device type: Doorbell, Signaler, or LabelScanner
DEVICE_MODE=Doorbell

# IP address of the orchestrator server on local network
ORCHESTRATOR_HOST=192.168.1.100

# Port the orchestrator is listening on
ORCHESTRATOR_PORT=9999
